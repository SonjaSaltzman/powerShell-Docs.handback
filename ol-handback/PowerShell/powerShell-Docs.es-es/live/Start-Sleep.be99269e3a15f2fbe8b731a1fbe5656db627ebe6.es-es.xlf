        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>Start-Sleep</source>
          <target state="translated">Start-Sleep</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Start-Sleep</source>
          <target state="translated">Start-Sleep</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>SYNOPSIS</source>
          <target state="translated">SINOPSIS</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Suspends the activity in a script or session for the specified period of time.</source>
          <target state="translated">Suspende la actividad en un script o en una sesión durante el período de tiempo especificado.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>SYNTAX</source>
          <target state="translated">SINTAXIS</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>Seconds (Default)</source>
          <target state="translated">Segundos (valor predeterminado)</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>Milliseconds</source>
          <target state="translated">Milisegundos</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>DESCRIPTION</source>
          <target state="translated">DESCRIPCIÓN</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>The <bpt id="p1">**</bpt>Start-Sleep<ept id="p1">**</ept> cmdlet suspends the activity in a script or session for the specified period of time.</source>
          <target state="translated">El <bpt id="p1">**</bpt>Start-Sleep<ept id="p1">**</ept> cmdlet suspende la actividad en una secuencia de comandos o una sesión para el período de tiempo especificado.</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>You can use it for many tasks, such as waiting for an operation to complete or pausing before repeating an operation.</source>
          <target state="translated">Se puede usar para muchas tareas, como esperar a que se complete una operación o pausar antes de repetir una operación.</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>EXAMPLES</source>
          <target state="translated">EJEMPLOS</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve">
          <source>Example 1: Sleep all commands for 15 seconds</source>
          <target state="translated">Ejemplo 1: Todos los comandos de suspensión durante 15 segundos</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve">
          <source>This command makes all commands in the session sleep for 15 seconds.</source>
          <target state="translated">Este comando suspende todos los comandos de la sesión durante 15 segundos.</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve">
          <source>Example 2: Sleep all commands</source>
          <target state="translated">Ejemplo 2: Suspensión de todos los comandos</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve">
          <source>This command makes all the commands in the session sleep for one-half of a second (500 milliseconds).</source>
          <target state="translated">Este comando suspende todos los comandos de la sesión durante medio segundo (500 milisegundos).</target>       </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve">
          <source>PARAMETERS</source>
          <target state="translated">PARÁMETROS</target>       </trans-unit>
        <trans-unit id="118" translate="yes" xml:space="preserve">
          <source>-Milliseconds</source>
          <target state="translated">-Milisegundos</target>       </trans-unit>
        <trans-unit id="119" translate="yes" xml:space="preserve">
          <source>Specifies how long the resource sleeps in milliseconds.</source>
          <target state="translated">Especifica en milisegundos cuánto tiempo se suspende el recurso.</target>       </trans-unit>
        <trans-unit id="120" translate="yes" xml:space="preserve">
          <source>The parameter can be abbreviated as <bpt id="p1">*</bpt>m<ept id="p1">*</ept>.</source>
          <target state="translated">El parámetro se puede abreviar como <bpt id="p1">*</bpt>m<ept id="p1">*</ept>.</target>       </trans-unit>
        <trans-unit id="121" translate="yes" xml:space="preserve">
          <source>-Seconds</source>
          <target state="translated">-Segundos</target>       </trans-unit>
        <trans-unit id="122" translate="yes" xml:space="preserve">
          <source>Specifies how long the resource sleeps in seconds.</source>
          <target state="translated">Especifica en segundos cuánto tiempo se suspende el recurso.</target>       </trans-unit>
        <trans-unit id="123" translate="yes" xml:space="preserve">
          <source>You can omit the parameter name (<bpt id="p1">*</bpt>Seconds<ept id="p1">*</ept>), or you can abbreviate it as <bpt id="p2">*</bpt>s<ept id="p2">*</ept>.</source>
          <target state="translated">Puede omitir el nombre de parámetro (<bpt id="p1">*</bpt>segundos<ept id="p1">*</ept>), o se puede abreviar como <bpt id="p2">*</bpt>s<ept id="p2">*</ept>.</target>       </trans-unit>
        <trans-unit id="124" translate="yes" xml:space="preserve">
          <source>CommonParameters</source>
          <target state="translated">CommonParameters</target>       </trans-unit>
        <trans-unit id="125" translate="yes" xml:space="preserve">
          <source>This cmdlet supports the common parameters: -Debug, -ErrorAction, -ErrorVariable, -InformationAction, -InformationVariable, -OutVariable, -OutBuffer, -PipelineVariable, -Verbose, -WarningAction, and -WarningVariable.</source>
          <target state="translated">Este cmdlet admite los parámetros comunes:-Debug, - ErrorAction, - ErrorVariable, - InformationAction, - InformationVariable, - OutVariable,-OutBuffer, - PipelineVariable, - Verbose, - WarningAction y - WarningVariable.</target>       </trans-unit>
        <trans-unit id="126" translate="yes" xml:space="preserve">
          <source>For more information, see about_CommonParameters (http://go.microsoft.com/fwlink/?LinkID=113216).</source>
          <target state="translated">Para obtener más información, consulte about_CommonParameters (http://go.microsoft.com/fwlink/?LinkID=113216).</target>       </trans-unit>
        <trans-unit id="127" translate="yes" xml:space="preserve">
          <source>INPUTS</source>
          <target state="translated">ENTRADAS</target>       </trans-unit>
        <trans-unit id="128" translate="yes" xml:space="preserve">
          <source>System.Int32</source>
          <target state="translated">System.Int32</target>       </trans-unit>
        <trans-unit id="129" translate="yes" xml:space="preserve">
          <source>You can pipe the number of seconds to <bpt id="p1">**</bpt>Start-Sleep<ept id="p1">**</ept>.</source>
          <target state="translated">Puede canalizar el número de segundos de <bpt id="p1">**</bpt>Start-Sleep<ept id="p1">**</ept>.</target>       </trans-unit>
        <trans-unit id="130" translate="yes" xml:space="preserve">
          <source>OUTPUTS</source>
          <target state="translated">SALIDAS</target>       </trans-unit>
        <trans-unit id="131" translate="yes" xml:space="preserve">
          <source>None</source>
          <target state="translated">Ninguno</target>       </trans-unit>
        <trans-unit id="132" translate="yes" xml:space="preserve">
          <source>This cmdlet does not return any output.</source>
          <target state="translated">Este cmdlet no genera resultados de ningún tipo.</target>       </trans-unit>
        <trans-unit id="133" translate="yes" xml:space="preserve">
          <source>NOTES</source>
          <target state="translated">NOTAS</target>       </trans-unit>
        <trans-unit id="134" translate="yes" xml:space="preserve">
          <source>You can also refer to <bpt id="p1">**</bpt>Start-Sleep<ept id="p1">**</ept> by its built-in alias, <bpt id="p2">**</bpt>sleep<ept id="p2">**</ept>.</source>
          <target state="translated">También puede hacer referencia a <bpt id="p1">**</bpt>Start-Sleep<ept id="p1">**</ept> por su alias integrado, <bpt id="p2">**</bpt>suspensión<ept id="p2">**</ept>.</target>       </trans-unit>
