        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>Deletes the content in the specified alternate data stream, but does not delete the alternate data stream.</source>
          <target state="translated">Elimina il contenuto del flusso di dati alternativo specificato, ma non elimina il flusso di dati alternativo.</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve">
