        <trans-unit id="122" translate="yes" xml:space="preserve">
          <source>None</source>
          <target state="translated">Nessuno</target>       </trans-unit>
        <trans-unit id="123" translate="yes" xml:space="preserve">
          <source>Clear-Host does not generate any output</source>
          <target state="translated">Clear-Host non genera alcun output.</target>       </trans-unit>
        <trans-unit id="124" translate="yes" xml:space="preserve">
          <source>NOTES</source>
          <target state="translated">NOTE</target>       </trans-unit>
        <trans-unit id="125" translate="yes" xml:space="preserve">
          <source>Clear-Host is a simple function, not an advanced function.</source>
          <target state="translated">Clear-Host è una funzione semplice, non avanzata.</target>       </trans-unit>
        <trans-unit id="126" translate="yes" xml:space="preserve">
          <source>As such, you cannot use common parameters, such as -Debug, in a Clear-Host command.</source>
          <target state="translated">Di conseguenza, non è possibile usare parametri comuni, come -Debug, in un comando Clear-Host.</target>       </trans-unit>
