          <target state="translated">-------------------------- ESEMPIO 1 --------------------------</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve">
          <source>Description</source>
          <target state="translated">Descrizione</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve">
          <source>This command uses the CLS alias of Clear-Host to clear the current display.</source>
          <target state="translated">Questo comando usa l'alias CLS di Clear-Host per cancellare la visualizzazione corrente.</target>       </trans-unit>
