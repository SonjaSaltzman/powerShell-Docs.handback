        <trans-unit id="114" translate="yes" xml:space="preserve">
          <source>Example 2: Get all package sources for a specific provider</source>
          <target state="translated">Esempio 2: Ottenere tutte le origini del pacchetto per un determinato provider</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve">
