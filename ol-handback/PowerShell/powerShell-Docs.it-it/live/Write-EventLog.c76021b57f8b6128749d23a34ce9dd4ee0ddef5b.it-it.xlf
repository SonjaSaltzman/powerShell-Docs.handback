      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>powershell, cmdlet</source>
          <target state="translated">cmdlet PowerShell</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>Write-EventLog</source>
          <target state="translated">Write-EventLog</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Write-EventLog</source>
          <target state="translated">Write-EventLog</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>SYNOPSIS</source>
          <target state="translated">RIEPILOGO</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Writes an event to an event log.</source>
          <target state="translated">Scrive un evento in un registro eventi.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>SYNTAX</source>
          <target state="translated">SINTASSI</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>DESCRIPTION</source>
          <target state="translated">DESCRIZIONE</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>The Write-EventLog cmdlet writes an event to an event log.</source>
          <target state="translated">Il cmdlet Write-EventLog scrive un evento in un registro eventi.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>To write an event to an event log, the event log must exist on the computer and the source must be registered for the event log.</source>
          <target state="translated">Per scrivere un evento in un registro eventi, è necessario che il registro eventi sia presente nel computer e che l'origine sia registrata per il registro eventi.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>The cmdlets that contain the EventLog noun (the EventLog cmdlets) work only on classic event logs.</source>
          <target state="translated">I cmdlet che contengono il sostantivo EventLog (cmdlet EventLog) funzionano solo sui registri eventi classici.</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>To get events from logs that use the Windows Event Log technology in Windows Vista and later versions of Windows, use Get-WinEvent.</source>
          <target state="translated">Per ottenere eventi dai registri che usano la tecnologia del registro eventi di Windows in Windows Vista e versioni successive di Windows, usare Get-WinEvent.</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>EXAMPLES</source>
          <target state="translated">ESEMPI</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve">
          <source>-------------------------- EXAMPLE 1 --------------------------</source>
          <target state="translated">-------------------------- ESEMPIO 1 --------------------------</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve">
          <source>This command writes an event from the MyApp source to the Application event log.</source>
          <target state="translated">Questo comando scrive un evento generato dall'origine MyApp nel registro eventi dell'applicazione.</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve">
          <source>-------------------------- EXAMPLE 2 --------------------------</source>
          <target state="translated">-------------------------- EXAMPLE 2 --------------------------</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve">
          <source>This command writes an event from the MyApp source to the Application event log on the Server01 remote computer.</source>
          <target state="translated">Questo comando scrive un evento generato dall'origine MyApp nel registro eventi dell'applicazione nel computer remoto Server01.</target>       </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve">
          <source>PARAMETERS</source>
          <target state="translated">PARAMETRI</target>       </trans-unit>
        <trans-unit id="118" translate="yes" xml:space="preserve">
          <source>-Category</source>
          <target state="translated">-Categoria</target>       </trans-unit>
        <trans-unit id="119" translate="yes" xml:space="preserve">
          <source>Specifies a task category for the event.</source>
          <target state="translated">Specifica una categoria di attività per l'evento.</target>       </trans-unit>
        <trans-unit id="120" translate="yes" xml:space="preserve">
          <source>Enter an integer that is associated with the strings in the category message file for the event log.</source>
          <target state="translated">Immettere un numero intero associato alle stringhe nel file di messaggi sule categorie per il registro eventi.</target>       </trans-unit>
        <trans-unit id="121" translate="yes" xml:space="preserve">
          <source>-ComputerName</source>
          <target state="translated">-ComputerName</target>       </trans-unit>
        <trans-unit id="122" translate="yes" xml:space="preserve">
          <source>Specifies a remote computer.</source>
          <target state="translated">Specifica un computer remoto.</target>       </trans-unit>
        <trans-unit id="123" translate="yes" xml:space="preserve">
          <source>The default is the local computer.</source>
          <target state="translated">Il valore predefinito è il computer locale.</target>       </trans-unit>
        <trans-unit id="124" translate="yes" xml:space="preserve">
          <source>Type the NetBIOS name, an Internet Protocol (IP) address, or a fully qualified domain name of a remote computer.</source>
          <target state="translated">Digitare il nome NetBIOS, un indirizzo IP (Internet Protocol) o un nome di dominio completo di un computer remoto.</target>       </trans-unit>
        <trans-unit id="125" translate="yes" xml:space="preserve">
          <source>This parameter does not rely on Windows PowerShell remoting.</source>
          <target state="translated">Questo parametro non si basa sulla comunicazione remota di Windows PowerShell.</target>       </trans-unit>
        <trans-unit id="126" translate="yes" xml:space="preserve">
          <source>You can use the ComputerName parameter of Get-EventLog even if your computer is not configured to run remote commands.</source>
          <target state="translated">È possibile usare il parametro ComputerName di Get-EventLog anche se il computer non è configurato per eseguire comandi remoti.</target>       </trans-unit>
        <trans-unit id="127" translate="yes" xml:space="preserve">
          <source>-EntryType</source>
          <target state="translated">-EntryType</target>       </trans-unit>
        <trans-unit id="128" translate="yes" xml:space="preserve">
          <source>Specifies the entry type of the event.</source>
          <target state="translated">Specifica il tipo di voce dell'evento.</target>       </trans-unit>
        <trans-unit id="129" translate="yes" xml:space="preserve">
          <source>Valid values are Error, Warning, Information, SuccessAudit, and FailureAudit.</source>
          <target state="translated">I valori validi sono Error, Warning, Information, SuccessAudit e FailureAudit.</target>       </trans-unit>
        <trans-unit id="130" translate="yes" xml:space="preserve">
          <source>The default value is Information.</source>
          <target state="translated">Il valore predefinito è Information.</target>       </trans-unit>
        <trans-unit id="131" translate="yes" xml:space="preserve">
          <source>For a description of the values, see System.Diagnostics.EventLogEntryType in the MSDN (Microsoft Developer Network) library at http://go.microsoft.com/fwlink/?LinkId=143599.</source>
          <target state="translated">Per una descrizione dei valori, vedere System.Diagnostics.EventLogEntryType in MSDN (Microsoft Developer Network) library all'http://go.microsoft.com/fwlink/?LinkId=143599.</target>       </trans-unit>
        <trans-unit id="132" translate="yes" xml:space="preserve">
          <source>-EventId</source>
          <target state="translated">ID evento:</target>       </trans-unit>
        <trans-unit id="133" translate="yes" xml:space="preserve">
          <source>Specifies the event identifier.</source>
          <target state="translated">Specifica l'identificatore evento.</target>       </trans-unit>
        <trans-unit id="134" translate="yes" xml:space="preserve">
          <source>This parameter is required.</source>
          <target state="translated">Questo parametro è obbligatorio.</target>       </trans-unit>
        <trans-unit id="135" translate="yes" xml:space="preserve">
          <source>-LogName</source>
          <target state="translated">-LogName</target>       </trans-unit>
        <trans-unit id="136" translate="yes" xml:space="preserve">
          <source>Specifies the name of the log to which the event is written.</source>
          <target state="translated">Specifica il nome del registro in cui viene scritto l'evento.</target>       </trans-unit>
        <trans-unit id="137" translate="yes" xml:space="preserve">
          <source>Enter the log name (the value of the Log property, not the LogDisplayName).</source>
          <target state="translated">Immettere il nome del registro (il valore della proprietà Log, non LogDisplayName).</target>       </trans-unit>
        <trans-unit id="138" translate="yes" xml:space="preserve">
          <source>Wildcard characters are not permitted.</source>
          <target state="translated">I caratteri jolly non sono consentiti.</target>       </trans-unit>
        <trans-unit id="139" translate="yes" xml:space="preserve">
          <source>This parameter is required.</source>
          <target state="translated">Questo parametro è obbligatorio.</target>       </trans-unit>
        <trans-unit id="140" translate="yes" xml:space="preserve">
          <source>-Message</source>
          <target state="translated">-Messaggio</target>       </trans-unit>
        <trans-unit id="141" translate="yes" xml:space="preserve">
          <source>Specifies the event message.</source>
          <target state="translated">Specifica il messaggio di evento.</target>       </trans-unit>
        <trans-unit id="142" translate="yes" xml:space="preserve">
          <source>This parameter is required.</source>
          <target state="translated">Questo parametro è obbligatorio.</target>       </trans-unit>
        <trans-unit id="143" translate="yes" xml:space="preserve">
          <source>-RawData</source>
          <target state="translated">-RawData</target>       </trans-unit>
        <trans-unit id="144" translate="yes" xml:space="preserve">
          <source>Specifies the binary data that is associated with the event, in bytes.</source>
          <target state="translated">Specifica i dati binari associati all'evento, in byte.</target>       </trans-unit>
        <trans-unit id="145" translate="yes" xml:space="preserve">
          <source>-Source</source>
          <target state="translated">-Source</target>       </trans-unit>
        <trans-unit id="146" translate="yes" xml:space="preserve">
          <source>Specifies the event source, which is typically the name of the application that is writing the event to the log.</source>
          <target state="translated">Specifica l'origine evento, in genere il nome dell'applicazione che scrive l'evento nel registro.</target>       </trans-unit>
        <trans-unit id="147" translate="yes" xml:space="preserve">
          <source>CommonParameters</source>
          <target state="translated">CommonParameters</target>       </trans-unit>
        <trans-unit id="148" translate="yes" xml:space="preserve">
          <source>This cmdlet supports the common parameters: -Debug, -ErrorAction, -ErrorVariable, -InformationAction, -InformationVariable, -OutVariable, -OutBuffer, -PipelineVariable, -Verbose, -WarningAction, and -WarningVariable.</source>
          <target state="translated">Questo cmdlet supporta i parametri comuni:-eseguire il Debug, - ErrorAction, - ErrorVariable, - InformationAction, InformationVariable-, - OutVariable,-OutBuffer, - PipelineVariable, - Verbose, - WarningAction e - WarningVariable.</target>       </trans-unit>
        <trans-unit id="149" translate="yes" xml:space="preserve">
          <source>For more information, see about_CommonParameters (http://go.microsoft.com/fwlink/?LinkID=113216).</source>
          <target state="translated">Per ulteriori informazioni, vedere about_CommonParameters (http://go.microsoft.com/fwlink/?LinkID=113216).</target>       </trans-unit>
        <trans-unit id="150" translate="yes" xml:space="preserve">
          <source>INPUTS</source>
          <target state="translated">INPUT</target>       </trans-unit>
        <trans-unit id="151" translate="yes" xml:space="preserve">
          <source>None</source>
          <target state="translated">Nessuno</target>       </trans-unit>
        <trans-unit id="152" translate="yes" xml:space="preserve">
          <source>You cannot pipe input to this cmdlet.</source>
          <target state="translated">Non è possibile inviare input tramite pipe a questo cmdlet.</target>       </trans-unit>
        <trans-unit id="153" translate="yes" xml:space="preserve">
          <source>OUTPUTS</source>
          <target state="translated">OUTPUT</target>       </trans-unit>
        <trans-unit id="154" translate="yes" xml:space="preserve">
          <source>System.Diagnostics.EventLogEntry</source>
          <target state="translated">System.Diagnostics.EventLogEntry</target>       </trans-unit>
        <trans-unit id="155" translate="yes" xml:space="preserve">
          <source>Write-EventLog returns objects that represents the events in the logs.</source>
          <target state="translated">Write-EventLog restituisce gli oggetti che rappresentano gli eventi nei registri.</target>       </trans-unit>
        <trans-unit id="156" translate="yes" xml:space="preserve">
          <source>NOTES</source>
          <target state="translated">NOTE</target>       </trans-unit>
        <trans-unit id="157" translate="yes" xml:space="preserve">
          <source>To use Write-EventLog, start Windows PowerShell with the "Run as administrator" option.</source>
          <target state="translated">Per usare Write-EventLog, avviare Windows PowerShell con l'opzione "Esegui come amministratore".</target>       </trans-unit>
        <trans-unit id="158" translate="yes" xml:space="preserve">
          <source>RELATED LINKS</source>
          <target state="translated">COLLEGAMENTI CORRELATI</target>       </trans-unit>
