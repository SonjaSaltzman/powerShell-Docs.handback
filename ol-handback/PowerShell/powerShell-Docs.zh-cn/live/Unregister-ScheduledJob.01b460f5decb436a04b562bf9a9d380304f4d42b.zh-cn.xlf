          <source>-Name</source>
          <target state="translated">-Name</target>       </trans-unit>
        <trans-unit id="145" translate="yes" xml:space="preserve">
          <source>Deletes the scheduled jobs with the specified names.</source>
          <target state="translated">删除具有指定名称的计划作业。</target>       </trans-unit>
        <trans-unit id="146" translate="yes" xml:space="preserve">
          <source>Enter the names of one or more scheduled jobs on the computer.</source>
