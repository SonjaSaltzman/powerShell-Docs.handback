        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Writes text to the verbose message stream.</source>
          <target state="translated">将文本写入详细消息流。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
