        <trans-unit id="115" translate="yes" xml:space="preserve">
          <source>Specifies one or more strings to be joined.</source>
          <target state="translated">指定要联接的一个或多个字符串。</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve">
