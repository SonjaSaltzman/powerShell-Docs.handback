        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>The <bpt id="p1">**</bpt>Get-PSRepository<ept id="p1">**</ept> cmdlet gets PowerShell module repositories that are registered for the current user.</source>
          <target state="translated"> <bpt id="p1">**</bpt>Get PSRepository<ept id="p1">**</ept> 指令程式會取得目前使用者已註冊的 PowerShell 模組儲存機制。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>EXAMPLES</source>
          <target state="translated">範例</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>Example 1: Get all module repositories</source>
          <target state="translated">範例 1︰ 取得所有模組儲存機制</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>This command gets all module repositories registered for the current user.</source>
          <target state="translated">此命令會取得目前的使用者註冊的所有模組儲存機制。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>Example 2: Get module repositories by name</source>
          <target state="translated">範例 2︰ 依名稱取得模組儲存機制</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve">
          <source>This command gets all module repositories that include NuGet in their names.</source>
          <target state="translated">此命令會取得所有模組的儲存機制，其名稱中包含 NuGet。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve">
