        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Deletes certificate stores, certificates, and private keys.</source>
          <target state="translated">Elimina los almacenes de certificados, los certificados y las claves privadas.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Syntax</source>
          <target state="translated">Sintaxis</target>       </trans-unit>
