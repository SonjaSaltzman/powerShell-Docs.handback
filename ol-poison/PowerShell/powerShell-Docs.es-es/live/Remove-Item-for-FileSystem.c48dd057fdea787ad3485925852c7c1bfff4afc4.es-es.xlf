        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>Deletes the specified alternate data stream from a file, but does not delete the file.</source>
          <target state="translated">Elimina la secuencia de datos alternativa especificada de un archivo, pero no elimina el archivo.</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve">
