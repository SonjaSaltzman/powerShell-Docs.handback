          <target state="translated">Remove-Item para el sistema de archivos</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Deletes files and folders.</source>
          <target state="translated">Elimina archivos y carpetas.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Syntax</source>
          <target state="translated">Sintaxis</target>       </trans-unit>
