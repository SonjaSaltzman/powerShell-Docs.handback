        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>SYNOPSIS</source>
          <target state="translated">SINOPSIS</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Deletes a variable and its value.</source>
          <target state="translated">Elimina una variable y su valor.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>SYNTAX</source>
          <target state="translated">SINTAXIS</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>DESCRIPTION</source>
          <target state="translated">DESCRIPCIÓN</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>The <bpt id="p1">**</bpt>Remove-Variable<ept id="p1">**</ept> cmdlet deletes a variable and its value from the scope in which it is defined, such as the current session.</source>
          <target state="translated">El <bpt id="p1">**</bpt>Remove-Variable<ept id="p1">**</ept> cmdlet elimina una variable y su valor desde el ámbito en el que se define, por ejemplo, la sesión actual.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>You cannot use this cmdlet to delete variables that are set as constants or those that are owned by the system.</source>
          <target state="translated">No se puede usar este cmdlet para eliminar las variables que se definen como constantes o que son propiedad del sistema.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>EXAMPLES</source>
          <target state="translated">EJEMPLOS</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>Example 1: Remove a variable</source>
          <target state="translated">Ejemplo 1: Quitar una variable</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>This command deletes the $Smp variable.</source>
          <target state="translated">Este comando elimina la variable $Smp.</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve">
          <source>PARAMETERS</source>
          <target state="translated">PARÁMETROS</target>       </trans-unit>
