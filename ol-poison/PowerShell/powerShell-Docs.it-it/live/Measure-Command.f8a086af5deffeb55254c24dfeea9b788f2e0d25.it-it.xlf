        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Measures the time it takes to run script blocks and cmdlets.</source>
          <target state="translated">Misura il tempo necessario per eseguire i cmdlet e i blocchi di script.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
