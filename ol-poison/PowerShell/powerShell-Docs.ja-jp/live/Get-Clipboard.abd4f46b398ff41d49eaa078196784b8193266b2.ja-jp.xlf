        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>SYNOPSIS</source>
          <target state="translated">概要</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Gets the current Windows clipboard entry.</source>
          <target state="translated">現在の Windows クリップボードのエントリを取得します。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>SYNTAX</source>
          <target state="translated">構文</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>DESCRIPTION</source>
          <target state="translated">説明</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>The <bpt id="p1">**</bpt>Get-Clipboard<ept id="p1">**</ept> cmdlet gets the current Windows clipboard entry.</source>
          <target state="translated"> <bpt id="p1">**</bpt>Get クリップボード<ept id="p1">**</ept> コマンドレットは、現在の Windows クリップボードのエントリを取得します。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>EXAMPLES</source>
          <target state="translated">例</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>Example 1: Get the content of the clipboard and display it to the command-line</source>
          <target state="translated">例 1: クリップボードの内容を取得し、それをコマンドラインに表示</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>This command displays the contents of the clipboard to the command-line.</source>
          <target state="translated">このコマンドは、コマンドラインをクリップボードの内容を表示します。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>Example 2: Get the content of the clipboard and display it to the command-line</source>
          <target state="translated">例 2: クリップボードの内容を取得し、それをコマンドラインに表示</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve">
          <source>This command displays the link, as a URL, of the image that is stored in the clipboard.</source>
          <target state="translated">このコマンドは、クリップボードに格納されているイメージの URL として、リンクを表示します。</target>       </trans-unit>
